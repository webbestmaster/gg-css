{
  "name": "typescript-project",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webbestmaster/typescript-project.git"
  },
  "bugs": {
    "url": "https://github.com/webbestmaster/typescript-project/issues"
  },
  "homepage": "https://github.com/webbestmaster/typescript-project#readme",
  "license": "MIT",
  "author": "",
  "version": "0.0.0",
  "description": "",
  "keywords": [
    "typescript-project",
    "empty"
  ],
  "main": "dist/index.js",
  "types": "dist/library.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "front:start-dev": "cross-env NODE_ENV=development SIDE=front webpack-cli serve --progress",
    "front:start-dev:transpile-only": "cross-env NODE_ENV=development SIDE=front TS_TRANSPILE_ONLY=true webpack-cli serve --progress",
    "front:build": "cross-env NODE_ENV=production SIDE=front NODE_OPTIONS=--max-old-space-size=256 webpack --progress",
    "check": "npm run ts-coverage && npm run tsc && npm run eslint && npm run stylelint",
    "check:fix": "npm run prettier && npm run ts-coverage && npm run tsc && npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "stylelint": "stylelint ./**.scss",
    "stylelint:fix": "stylelint ./**.scss --fix",
    "prettier": "prettier --write \"./**/*.(js|ts|tsx)\" --loglevel warn",
    "tsc": "tsc -p ./tsconfig.json",
    "tsc:watch": "tsc -p ./tsconfig.json --watch",
    "front:ts-coverage": "typescript-coverage-report --threshold=100.00 --strict=true --project ./www/",
    "ts-coverage": "npm run front:ts-coverage",
    "test:e2e": "jest --config='test-e2e/config/jest.config.js' ./test-e2e/ --runInBand --silent --passWithNoTests",
    "test:unit": "jest --config='test-unit/config/jest.config.js' ./www/ --coverage --silent --passWithNoTests",
    "pre-commit-hook": "npm run check"
  },
  "dependencies": {
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "ajv": "^8.11.0",
    "markdown-pro": "^1.0.25",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-localization-library": "^0.0.19",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-system-hook": "^1.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-transform-strict-mode": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "babel-loader": "^8.2.5",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "css-modules-typescript-loader": "^4.0.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^8.20.0",
    "eslint-import-resolver-typescript": "^3.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sonarjs": "^0.14.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^43.0.2",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.19",
    "postcss": "^8.4.14",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "puppeteer": "^15.5.0",
    "raw-loader": "^4.0.2",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "script-ext-html-webpack-plugin": "^2.1.5",
    "style-loader": "^3.3.1",
    "stylelint": "^14.9.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-scss": "^4.3.0",
    "terser-webpack-plugin": "^5.3.3",
    "ts-jest": "^28.0.7",
